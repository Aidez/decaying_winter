local function u95(p94, p95, p96)
	local v690 = p96;
	local v691 = Color3.new(0, 0.8, 0.9);
	if p94.Parent.Name == "Heavy Ammo" or p94.Parent.Name == "Shells Ammo" or p94.Parent.Name == "Light Ammo" or p94.Parent.Name == "Long Ammo" or p94.Parent.Name == "Short Ammo" or p94.Parent.Name == "Medium Ammo" or p94.Parent.Name == "Small Ammo" then
		v691 = Color3.new(1, 0.8, 0);
	elseif p94.Name == "HarvestWood" or p94.Name == "HarvestStone" then
		v691 = Color3.new(0.8, 0, 0.8);
	elseif p94.Name == "JointGrip" then
		v691 = Color3.new(0.2, 0.9, 0);
	elseif p94.Name == "Head" then
		v691 = Color3.new(1, 0.1, 0);
	end;
	if p95 == true then
		v690 = 8;
	end;
	local v692 = l__game__6.ReplicatedStorage.misc.teammateindicator:Clone();
	l__Debris__32:AddItem(v692, v690);
	v692.AlwaysOnTop = true;
	v692.ImageLabel.ImageColor3 = v691;
	v692.Parent = p94;
	v692.Adornee = p94;
	if p95 == true then
		v692.ImageLabel.Rotation = 180;
		delay(p96 - 0.5, function()
			while true do
				v692.ImageLabel.ImageTransparency = v692.ImageLabel.ImageTransparency + 0.05;
				RS:wait();
				if v692 == nil then
					break;
				end;
				if v692.ImageLabel.ImageTransparency >= 1 then
					break;
				end;			
			end;
			v692:Destroy();
		end);
	end;
end;
