				v1274 = u102[p129];
			end;
			tauntanimset[p129]:Play(v1274, 1, v1275);
			tptauntanimset[p129]:Play(v1274, 1, v1275);
			if u102[p129] ~= nil then
				u103 = true;
				u8.action = true;
				u8.channel = true;
				while true do
					if tauntanimset[p129]:GetTimeOfKeyframe("Stop") <= tauntanimset[p129].TimePosition then
						tauntanimset[p129]:AdjustSpeed(0);
						tptauntanimset[p129]:AdjustSpeed(0);
					end;
					RS:wait();
					if u8.staggering == true then
						break;
					end;
					if u103 == false then
						break;
					end;				
				end;
				if u8.staggering == false then
					u8.action = false;
					u8.channel = false;
				end;
				u103 = false;
				tauntanimset[p129]:Stop(0.5);
				tptauntanimset[p129]:Stop(0.5);
			end;
			return;
		end;
	end;
end;
