		if v548:IsA("BasePart") then
			local v549 = v543:Clone();
			v549.Enabled = true;
			v549.Parent = v548;
		elseif v548 ~= v543 then
			if not v548:IsA("Folder") then
				v548:Destroy();
			end;
		end;	
	end;
	v542.CFrame = l__CurrentCamera__1.CFrame;
	v542.Parent = l__CurrentCamera__1;
	local v550 = Instance.new("Motor6D");
	v550.Part0 = v542;
	v550.Part1 = cpart;
	v550.C1 = CFrame.new(0, 0, -0.4) * CFrame.Angles(0, 0, 0);
	v550.Parent = v542;
	if p70 then
		delay(p70, function()
			local v551, v552, v553 = ipairs(v542:GetDescendants());
			while true do
				local v554, v555 = v551(v552, v553);
				if v554 then

				else
					break;
				end;
				v553 = v554;
