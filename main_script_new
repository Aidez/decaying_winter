local u103 = false;
local function u104(p104, p105)
	p104.status.Visible = true;
	local v997 = tick();
	while true do
		p104.hunted.Position = p104.hunted.Position:Lerp(UDim2.new(0.5, 0, 0.2, 0), 0.1);
		p104.focus.Position = p104.focus.Position:Lerp(UDim2.new(1.5, 0, 0.6, 0), 0.3);
		p104.status.Position = p104.status.Position:Lerp(UDim2.new(0.5, 0, 1, 0), 0.1);
		RS:wait();
		if tick() - v997 >= 2 then
			break;
		end;	
	end;
	playerstats = l__workspace__1.ServerStuff.retrieveStats:InvokeServer();
	if p105 == "loss" then
		p104.status.spectate.Visible = false;
		p104.status.menu.Visible = true;
		local v998 = { "WHAT THE HELL WERE YOU DOING?", "I'M BEWILDERED", "THAT'S JUST SAD", "UNFORTUNATE", "YOU DISAPPOINT ME", "HOW DID YOU MANAGE THAT?", "IMPRESSIVE", "SMOOTH MOVES", "EVERYONE MAKES MISTAKES", "THE MISTAKE HERE IS YOU", "UNFORTUNATE ACCIDENT", "THAT'S RATHER SAD", "DAMN", "WELL YOU TRIED", "YOU GET POINTS FOR TRYING" };
		p104.status.reports.killedby.eliminatedlevel.Text = v998[math.random(1, #v998)];
		if v121.killer ~= nil and (l__game__6.Players:FindFirstChild(v121.killer) or v121.killer == "Contestant") then
			local v999 = l__workspace__1.ServerStuff.relaylobbystat:InvokeServer(v121.killer);
			if v999 then
				local v1000 = v121.killer;
				local v1001 = v999.Level;
				if v999.Settings.Anonymous == true then
					if l__game__6.Players:FindFirstChild(v121.killer).Character and l__game__6.Players:FindFirstChild(v121.killer).Character:FindFirstChild("playerID") then
						local v1002 = "Contestant #" .. l__game__6.Players:FindFirstChild(v121.killer).Character:FindFirstChild("playerID").Value;
					end;
					v1000 = "Contestant";
					v1001 = 0;
				end;
				p104.status.reports.killedby.eliminated.Text = "ELIMINATED BY: " .. v1000;
				p104.status.reports.killedby.eliminatedlevel.Text = "LEVEL " .. v1001;
			else
				p104.status.reports.killedby.eliminated.Text = "ELIMINATED BY: Contestant";
				p104.status.reports.killedby.eliminatedlevel.Text = "LEVEL " .. 0;
			end;
		end;
	else
		p104.status.reports.killedby.eliminated.Text = "TOTAL WINS: " .. playerstats.MainStats.Wins;
		p104.status.reports.killedby.eliminatedlevel.Text = "WIN STREAK: " .. playerstats.MainStats.WinStreak;
	end;
	p104.status.reports.rankings.damage.Text = "MELEE DAMAGE DONE: " .. math.ceil(v121.damage);
	p104.status.reports.rankings.eliminations.Text = "ELIMINATIONS: " .. v121.kills;
	p104.status.reports.rankings.timealive.Text = "TIME ALIVE: " .. math.ceil((tick() - v121.timealive) / 60) .. "M";
	p104.status.reports.fansearned.amt.Text = "FANS EARNED: " .. playerstats.MatchXP;
	local v1003 = "ST";
	if v121.placed == 2 then
		v1003 = "ND";
	elseif v121.placed == 3 then
		v1003 = "RD";
	elseif v121.placed > 3 then
		v1003 = "TH";
	end;
	p104.status.reports.rankings.place.Text = "PLACED: " .. v121.placed .. v1003;
	if p105 == "siege" then
		p104.status.reports.rankings.place.Visible = false;
	end;
	local function v1004(p106)
		local v1005 = 0;
		local v1006 = tick();
		while true do
			p106.Visible = true;
			if tick() - v1006 >= 0.075 then
				v1006 = tick();
				v1005 = v1005 + 1;
				p106.Visible = false;
			end;
			RS:wait();
			if v1005 >= 5 then
				break;
			end;		
		end;
		p106.Visible = true;
	end;
	v1004(p104.status.reports.killedby);
	spawn(function()
		v1004(p104.status.reports.rankings);
	end);
	spawn(function()
		v1004(p104.status.reports.fansearned);
	end);
end;
