local function u86(p71)
	maingui.infectionalert.TextTransparency = 1;
	maingui.infectionalert.TextStrokeTransparency = maingui.infectionalert.TextTransparency;
	maingui.infectionalert.Text = "";
	agent_speak("viral");
	local u87 = "Systemic Infection has reached Stage " .. p71 .. ".";
	spawn(function()
		local v558 = tick();
		local v559 = 0;
		local v560 = tick();
		while true do
			maingui.infectionalert.TextTransparency = 1 - (tick() - v558) / 2;
			maingui.infectionalert.TextStrokeTransparency = maingui.infectionalert.TextTransparency;
			if tick() - v560 > 0.1 then
				v560 = tick();
				v559 = v559 + 1;
				maingui.infectionalert.Text = string.sub(u87, 1, v559);
				if string.len(u87) <= v559 then
					maingui.infectionalert.Text = u87;
				end;
			end;
			RS:wait();
			if maingui.infectionalert.Text == u87 then
				break;
			end;		
		end;
		maingui.infectionalert.TextTransparency = 0;
		maingui.infectionalert.TextStrokeTransparency = maingui.infectionalert.TextTransparency;
		local v561 = tick();
		while true do
			RS:wait();
			if tick() - v561 >= 2 then
				break;
			end;		
		end;
		local v562 = tick();
		while true do
			maingui.infectionalert.TextTransparency = (tick() - v562) / 2;
			maingui.infectionalert.TextStrokeTransparency = maingui.infectionalert.TextTransparency;
			RS:wait();
			if tick() - v562 >= 2 then
				break;
			end;		
		end;
		maingui.infectionalert.TextTransparency = 1;
		maingui.infectionalert.TextStrokeTransparency = maingui.infectionalert.TextTransparency;
	end);
end;
