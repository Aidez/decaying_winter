						maingui.LowerHudFrame.reserve.Visible = false;
						maingui.LowerHudFrame.altitem.Visible = false;
						if v992.ability ~= nil then
							maingui.LowerHudFrame.itemusage.Visible = true;
							local v996 = "";
							if v992.rarity ~= nil then
								v996 = "Tier " .. v992.rarity .. " / ";
							end;
							maingui.LowerHudFrame.itemusage.Text = v996 .. v992.ability;
							if v995 then
								maingui.LowerHudFrame.itemusage.Text = v996 .. v995;
							end;
						end;
					end;
					v991.BackgroundColor3 = Color3.new(0, 0, 0);
					v991.slotarea.BackgroundColor3 = Color3.new(0, 0, 0);
					v991.highlight.Visible = true;
					v991.slotarea.Size = UDim2.new(1, 0, 0, 45);
					maingui.LowerHudFrame.itemname.Text = v992.name;
					if v994 then
						maingui.LowerHudFrame.itemname.Text = v994;
					end;
				else
					v991.highlight.Visible = false;
					v991.BackgroundColor3 = Color3.new(1, 1, 1);
					v991.slotarea.Size = UDim2.new(1, 0, 0, 25);
					v991.slotarea.BackgroundColor3 = Color3.new(1, 1, 1);
				end;
			end;		
		end;
	end;
end;
