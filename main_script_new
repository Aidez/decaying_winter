local function u136(p129)
	if u8.action == false and u8.aiming == false and u8.channel == false and u8.firing == false then
		emotelist = false;
		maingui.Centre.emotes.Visible = false;
		if (p129 == "flourish" or p129 == "salute") and (u14.weapontype ~= "Bludgeon" and u14.weapontype ~= "Spear" and u14.weapontype ~= "Blade" and u14.weapontype ~= "Axe" or u14.animset == "Fists") then
			return;
		elseif p129 == "raise weapon" and u14.animset == "Fists" then
			return;
		else
			local v1274 = 0.1;
			local v1275 = 1;
			if p129 == "salute" or p129 == "cut throat" then
				v1275 = 0.65;
			end;
			if p129 == "flourish" then
				v1275 = 0.8;
			end;
