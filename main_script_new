local function u25()
	local v126 = nil;
	while true do
		v126 = tostring(math.random(0, 500));
		if u24[v126] == nil then
			break;
		end;	
	end;
	u24[v126] = CFrame.Angles(0, 0, 0);
	return v126;
end;
local function u26(p32, p33)
	for v127 = 1, p33.loops do
		for v128 = 1, p33.tables[v127].frames do
			u24[p32] = u24[p32]:lerp(p33.tables[v127].value, p33.tables[v127].speed);
			RS:wait();
		end;
		RS:wait();
	end;
	for v129 = 1, p33.returnframes do
		u24[p32] = u24[p32]:lerp(CFrame.Angles(0, 0, 0), p33.returnspeed);
		RS:wait();
	end;
	u24[p32] = nil;
end;
function shakecamera(p34, p35)
	spawn(function()
		local v130 = u25();
		if p34 == "weapon" then
			if p35[1] ~= "Fists" then
				if p35[1] == "BK" then
					if p35[2] == "swingvar1" then
						u26(v130, {
							loops = 1, 
							tables = { {
									frames = 5, 
									value = CFrame.Angles(0, math.rad(-7), math.rad(-7)), 
									speed = 0.15
								} }, 
							returnframes = 30, 
							returnspeed = 0.1
						});
					elseif p35[2] == "swingvar2" then
						u26(v130, {
							loops = 2, 
							tables = { {
									frames = 5, 
