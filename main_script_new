function afflictstatus(p72)
	if u9[p72.name] then
		u9[p72.name].effects.currentduration = tick();
		if p72.name ~= "Cripple" then
			if p72.name ~= "Bleed" then
				if p72.name ~= "Expose" then
					if p72.name == "Pierce" then
						if u9[p72.name].mainstats.intensity < p72.intensity then
							u9[p72.name].mainstats.intensity = p72.intensity;
							return;
						end;
					end;
				elseif u9[p72.name].mainstats.intensity < p72.intensity then
					u9[p72.name].mainstats.intensity = p72.intensity;
					return;
				end;
			elseif u9[p72.name].mainstats.intensity < p72.intensity then
				u9[p72.name].mainstats.intensity = p72.intensity;
				return;
			end;
		elseif u9[p72.name].mainstats.intensity < p72.intensity then
			u9[p72.name].mainstats.intensity = p72.intensity;
			return;
		end;
	else
		if playerperks.lead then
