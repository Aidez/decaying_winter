function shadowrespawnflash()
	if shadowForm == false then
		return;
	end;
	v104({ "gamemode" }, "shadowchosen", nil, nil, true, true);
	local v629 = Instance.new("Frame");
	v629.BorderSizePixel = 0;
	v629.BackgroundColor3 = Color3.new(0.9, 0, 0);
	v629.Size = UDim2.new(1, 0, 1, 0);
	v629.ZIndex = 100;
	v629.Parent = maingui;
	while true do
		v629.BackgroundTransparency = v629.BackgroundTransparency + 0.025;
		RS:wait();
		if not (1 <= v629.BackgroundTransparency) then

		else
			break;
		end;	
	end;
	v629:Destroy();
end;
